VAR myvar;
VAR count;
VAR ;

myvar=4;
 = 1234567890;

// should draw a rectangle
IF(myvar==4) THEN {
    GO 40;
    TURN 90;
    GO 40;
    TURN 90;
    GO 40;
    TURN 90;
    GO 40;
    TURN 90;
}

// reset turtle position
JUMP 50;

//should take ELSE and draw a triangle
IF(myvar>=7) THEN {
GO 40;
TURN 90;
GO 40;
